{
  "manifest_version" :2,
  "name": "Web Profiling - Cigital",
  "version": "1.0.1",
  "description": "This extension will gather profiling and scoping information for this page",
  
  "background": {
      "page":"WebProfiling/background.html"
  },
  
  "permissions": [
    "tabs",
	"notifications",
	"http://*/*",
	"https://*/*"
  ],
  
  "browser_action": {
    "default_title": "Inspect page",
    "default_icon": "WebProfiling/img/icon.png",
    "default_popup": "WebProfiling/popup.html"
  },
  
  "icons": {
    "16": "WebProfiling/img/icon.png",
    "48": "WebProfiling/img/icon.png",
    "128": "WebProfiling/img/icon.png"
  }
}
